{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		}
	},
	"globalDependencies": [".env"],
	"globalEnv": ["SENDGRID_API_KEY", "VITE_GITHUB_CLIENT_ID", "GITHUB_CLIENT_SECRET", "VITE_API_URL"]
}
