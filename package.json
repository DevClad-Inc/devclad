{
	"name": "devclad",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"@devclad/config": "*",
		"@devclad/ui": "*",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"turbo": "1.6.2"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"dependencies": {},
	"packageManager": "yarn@1.22.19"
}
